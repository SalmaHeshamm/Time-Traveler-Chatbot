22. \uc0\u1605 \u1593 \u1576 \u1583  \u1583 \u1606 \u1583 \u1585 \u1577  \u1608 \u1587 \u1602 \u1601  \u1575 \u1604 \u1571 \u1576 \u1585 \u1575 \u1580  \u1575 \u1604 \u1601 \u1604 \u1603 \u1610 \u1577 \
\uc0\u1605 \u1593 \u1576 \u1583  \u1581 \u1578 \u1581 \u1608 \u1585  \u1601 \u1610  \u1583 \u1606 \u1583 \u1585 \u1577  (\u1576 \u1615 \u1606 \u1610  \u1601 \u1610  \u1575 \u1604 \u1593 \u1589 \u1585  \u1575 \u1604 \u1576 \u1591 \u1604 \u1605 \u1610  \u1593 \u1604 \u1609  \u1605 \u1608 \u1602 \u1593  \u1605 \u1593 \u1576 \u1583  \u1602 \u1583 \u1610 \u1605 ) \u1610 \u1581 \u1578 \u1608 \u1610  \u1593 \u1604 \u1609  \u1608 \u1575 \u1581 \u1583 \u1577  \u1605 \u1606  \u1571 \u1588 \u1607 \u1585  \u1575 \u1604 \u1582 \u1585 \u1575 \u1574 \u1591  \u1575 \u1604 \u1601 \u1604 \u1603 \u1610 \u1577  \u1601 \u1610  \u1575 \u1604 \u1593 \u1575 \u1604 \u1605  \u1575 \u1604 \u1602 \u1583 \u1610 \u1605  - "\u1583 \u1575 \u1574 \u1585 \u1577  \u1583 \u1606 \u1583 \u1585 \u1577  \u1575 \u1604 \u1601 \u1604 \u1603 \u1610 \u1577 ". \u1575 \u1604 \u1587 \u1602 \u1601  \u1610 \u1615 \u1592 \u1607 \u1585  \u1582 \u1585 \u1610 \u1591 \u1577  \u1575 \u1604 \u1587 \u1605 \u1575 \u1569  \u1576 \u1575 \u1604 \u1571 \u1576 \u1585 \u1575 \u1580  \u1608 \u1575 \u1604 \u1603 \u1608 \u1575 \u1603 \u1576  \u1603 \u1605 \u1575  \u1593 \u1585 \u1601 \u1607 \u1575  \u1575 \u1604 \u1605 \u1589 \u1585 \u1610 \u1608 \u1606 .\
\uc0\u1575 \u1604 \u1582 \u1585 \u1610 \u1591 \u1577  \u1578 \u1589 \u1608 \u1585  12 \u1576 \u1585 \u1580 \u1611 \u1575  \u1601 \u1604 \u1603 \u1610 \u1611 \u1575  (\u1571 \u1589 \u1604  \u1575 \u1604 \u1571 \u1576 \u1585 \u1575 \u1580  \u1575 \u1604 \u1581 \u1583 \u1610 \u1579 \u1577 )\u1548  \u1608 \u1575 \u1604 \u1603 \u1608 \u1575 \u1603 \u1576  \u1575 \u1604 \u1582 \u1605 \u1587 \u1577  \u1575 \u1604 \u1605 \u1593 \u1585 \u1608 \u1601 \u1577  \u1604 \u1604 \u1593 \u1610 \u1606  \u1575 \u1604 \u1605 \u1580 \u1585 \u1583 \u1577  (\u1593 \u1591 \u1575 \u1585 \u1583 \u1548  \u1575 \u1604 \u1586 \u1607 \u1585 \u1577 \u1548  \u1575 \u1604 \u1605 \u1585 \u1610 \u1582 \u1548  \u1575 \u1604 \u1605 \u1588 \u1578 \u1585 \u1610 \u1548  \u1586 \u1581 \u1604 )\u1548  \u1608 36 \u1605 \u1580 \u1605 \u1608 \u1593 \u1577  \u1606 \u1580 \u1605 \u1610 \u1577  (\u1575 \u1604 \u1583 \u1610 \u1603 \u1575 \u1606 \u1575 \u1578 ) \u1575 \u1604 \u1578 \u1610  \u1575 \u1587 \u1578 \u1615 \u1582 \u1583 \u1605 \u1578  \u1604 \u1578 \u1602 \u1587 \u1610 \u1605  \u1575 \u1604 \u1587 \u1606 \u1577 .\
\uc0\u1575 \u1604 \u1605 \u1589 \u1585 \u1610 \u1608 \u1606  \u1575 \u1604 \u1602 \u1583 \u1605 \u1575 \u1569  \u1603 \u1575 \u1606 \u1608 \u1575  \u1601 \u1604 \u1603 \u1610 \u1610 \u1606  \u1576 \u1575 \u1585 \u1593 \u1610 \u1606 . \u1575 \u1587 \u1578 \u1582 \u1583 \u1605 \u1608 \u1575  \u1606 \u1580 \u1605  \u1575 \u1604 \u1588 \u1593 \u1585 \u1609  \u1575 \u1604 \u1610 \u1605 \u1575 \u1606 \u1610 \u1577  (\u1587 \u1608 \u1579 \u1610 \u1587 ) \u1604 \u1604 \u1578 \u1606 \u1576 \u1572  \u1576 \u1601 \u1610 \u1590 \u1575 \u1606  \u1575 \u1604 \u1606 \u1610 \u1604  \u1575 \u1604 \u1587 \u1606 \u1608 \u1610 . \u1591 \u1608 \u1585 \u1608 \u1575  \u1578 \u1602 \u1608 \u1610 \u1605 \u1611 \u1575  \u1588 \u1605 \u1587 \u1610 \u1611 \u1575  \u1605 \u1606  365 \u1610 \u1608 \u1605 \u1611 \u1575  (12 \u1588 \u1607 \u1585 \u1611 \u1575  \u1605 \u1606  30 \u1610 \u1608 \u1605 \u1611 \u1575  + 5 \u1571 \u1610 \u1575 \u1605  \u1573 \u1590 \u1575 \u1601 \u1610 \u1577 ). \u1576 \u1606 \u1608 \u1575  \u1575 \u1604 \u1605 \u1593 \u1575 \u1576 \u1583  \u1608 \u1575 \u1604 \u1571 \u1607 \u1585 \u1575 \u1605 \u1575 \u1578  \u1576 \u1605 \u1581 \u1575 \u1584 \u1575 \u1577  \u1601 \u1604 \u1603 \u1610 \u1577  \u1583 \u1602 \u1610 \u1602 \u1577  - \u1575 \u1604 \u1571 \u1607 \u1585 \u1575 \u1605 \u1575 \u1578  \u1575 \u1604 \u1579 \u1604 \u1575 \u1579 \u1577  \u1576 \u1575 \u1604 \u1580 \u1610 \u1586 \u1577  \u1605 \u1581 \u1575 \u1584 \u1610 \u1577  \u1604 \u1606 \u1580 \u1608 \u1605  \u1581 \u1586 \u1575 \u1605  \u1575 \u1604 \u1580 \u1576 \u1575 \u1585  (\u1571 \u1608 \u1585 \u1610 \u1608 \u1606 ) \u1578 \u1605 \u1575 \u1605 \u1611 \u1575 !\
