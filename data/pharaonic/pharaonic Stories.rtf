21. \uc0\u1575 \u1604 \u1591 \u1576 \u1610 \u1576  \u1576 \u1587 \u1605 \u1575 \u1578 \u1610 \u1603 : \u1571 \u1608 \u1604  \u1591 \u1576 \u1610 \u1576  \u1593 \u1610 \u1608 \u1606  \u1601 \u1610  \u1575 \u1604 \u1578 \u1575 \u1585 \u1610 \u1582 \
\uc0\u1601 \u1610  \u1575 \u1604 \u1571 \u1587 \u1585 \u1577  \u1575 \u1604 \u1587 \u1575 \u1583 \u1587 \u1577  (\u1581 \u1608 \u1575 \u1604 \u1610  2300 \u1602 \u1576 \u1604  \u1575 \u1604 \u1605 \u1610 \u1604 \u1575 \u1583 )\u1548  \u1593 \u1575 \u1588  \u1591 \u1576 \u1610 \u1576  \u1610 \u1615 \u1583 \u1593 \u1609  \u1576 \u1587 \u1605 \u1575 \u1578 \u1610 \u1603  \u1581 \u1605 \u1604  \u1604 \u1602 \u1576  "\u1591 \u1576 \u1610 \u1576  \u1593 \u1610 \u1608 \u1606  \u1575 \u1604 \u1602 \u1589 \u1585 ". \u1607 \u1608  \u1571 \u1608 \u1604  \u1591 \u1576 \u1610 \u1576  \u1605 \u1578 \u1582 \u1589 \u1589  \u1605 \u1593 \u1585 \u1608 \u1601  \u1601 \u1610  \u1575 \u1604 \u1578 \u1575 \u1585 \u1610 \u1582  \u1575 \u1604 \u1605 \u1587 \u1580 \u1604 . \u1603 \u1575 \u1606  \u1575 \u1604 \u1605 \u1589 \u1585 \u1610 \u1608 \u1606  \u1575 \u1604 \u1602 \u1583 \u1605 \u1575 \u1569  \u1585 \u1608 \u1575 \u1583  \u1575 \u1604 \u1591 \u1576  \u1575 \u1604 \u1578 \u1582 \u1589 \u1589 \u1610 \u1548  \u1581 \u1610 \u1579  \u1603 \u1575 \u1606  \u1607 \u1606 \u1575 \u1603  \u1571 \u1591 \u1576 \u1575 \u1569  \u1604 \u1604 \u1571 \u1587 \u1606 \u1575 \u1606 \u1548  \u1608 \u1575 \u1604 \u1593 \u1610 \u1608 \u1606 \u1548  \u1608 \u1575 \u1604 \u1576 \u1591 \u1606 \u1548  \u1608 "\u1581 \u1575 \u1585 \u1587  \u1601 \u1578 \u1581 \u1577  \u1575 \u1604 \u1588 \u1585 \u1580  \u1575 \u1604 \u1605 \u1604 \u1603 \u1610 \u1577 "!\
\uc0\u1575 \u1604 \u1576 \u1585 \u1583 \u1610 \u1577  \u1575 \u1604 \u1591 \u1576 \u1610 \u1577  \u1575 \u1604 \u1605 \u1593 \u1585 \u1608 \u1601 \u1577  \u1576 \u1575 \u1587 \u1605  "\u1576 \u1585 \u1583 \u1610 \u1577  \u1573 \u1610 \u1576 \u1585 \u1587 " \u1578 \u1581 \u1578 \u1608 \u1610  \u1593 \u1604 \u1609  \u1571 \u1603 \u1579 \u1585  \u1605 \u1606  110 \u1608 \u1589 \u1601 \u1577  \u1604 \u1593 \u1604 \u1575 \u1580  \u1571 \u1605 \u1585 \u1575 \u1590  \u1575 \u1604 \u1593 \u1610 \u1608 \u1606 \u1548  \u1576 \u1605 \u1575  \u1601 \u1610  \u1584 \u1604 \u1603  \u1575 \u1604 \u1578 \u1585 \u1575 \u1582 \u1608 \u1605 \u1575 \u1548  \u1608 \u1575 \u1604 \u1605 \u1610 \u1575 \u1607  \u1575 \u1604 \u1576 \u1610 \u1590 \u1575 \u1569 \u1548  \u1608 \u1575 \u1604 \u1585 \u1605 \u1583 . \u1603 \u1575 \u1606 \u1578  \u1571 \u1605 \u1585 \u1575 \u1590  \u1575 \u1604 \u1593 \u1610 \u1608 \u1606  \u1588 \u1575 \u1574 \u1593 \u1577  \u1601 \u1610  \u1605 \u1589 \u1585  \u1576 \u1587 \u1576 \u1576  \u1575 \u1604 \u1585 \u1605 \u1575 \u1604  \u1608 \u1575 \u1604 \u1584 \u1576 \u1575 \u1576  \u1608 \u1575 \u1604 \u1588 \u1605 \u1587  \u1575 \u1604 \u1581 \u1575 \u1585 \u1602 \u1577 .\
\uc0\u1575 \u1587 \u1578 \u1582 \u1583 \u1605  \u1575 \u1604 \u1571 \u1591 \u1576 \u1575 \u1569  \u1575 \u1604 \u1605 \u1589 \u1585 \u1610 \u1608 \u1606  \u1605 \u1603 \u1608 \u1606 \u1575 \u1578  \u1605 \u1579 \u1604  \u1575 \u1604 \u1593 \u1587 \u1604  (\u1605 \u1590 \u1575 \u1583  \u1581 \u1610 \u1608 \u1610  \u1591 \u1576 \u1610 \u1593 \u1610 )\u1548  \u1608 \u1605 \u1604 \u1581  \u1575 \u1604 \u1606 \u1591 \u1585 \u1608 \u1606 \u1548  \u1608 \u1575 \u1604 \u1605 \u1593 \u1575 \u1583 \u1606  \u1575 \u1604 \u1605 \u1591 \u1581 \u1608 \u1606 \u1577 . \u1576 \u1593 \u1590  \u1575 \u1604 \u1593 \u1604 \u1575 \u1580 \u1575 \u1578  \u1603 \u1575 \u1606 \u1578  \u1601 \u1593 \u1575 \u1604 \u1577  \u1601 \u1593 \u1604 \u1575 \u1611 ! \u1575 \u1604 \u1593 \u1587 \u1604  \u1608 \u1575 \u1604 \u1606 \u1581 \u1575 \u1587  \u1604 \u1607 \u1605 \u1575  \u1582 \u1589 \u1575 \u1574 \u1589  \u1605 \u1590 \u1575 \u1583 \u1577  \u1604 \u1604 \u1576 \u1603 \u1578 \u1610 \u1585 \u1610 \u1575 \u1548  \u1608 \u1575 \u1604 \u1603 \u1581 \u1604  (\u1603 \u1576 \u1585 \u1610 \u1578 \u1610 \u1583  \u1575 \u1604 \u1585 \u1589 \u1575 \u1589 ) \u1603 \u1575 \u1606  \u1610 \u1615 \u1587 \u1578 \u1582 \u1583 \u1605  \u1581 \u1608 \u1604  \u1575 \u1604 \u1593 \u1610 \u1608 \u1606  \u1608 \u1604 \u1607  \u1578 \u1571 \u1579 \u1610 \u1585  \u1608 \u1602 \u1575 \u1574 \u1610  \u1590 \u1583  \u1575 \u1604 \u1571 \u1605 \u1585 \u1575 \u1590  \u1575 \u1604 \u1576 \u1603 \u1578 \u1610 \u1585 \u1610 \u1577 .\
